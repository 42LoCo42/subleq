# loop:
# 	address of cell to modify
# 	address of cell with target value
# 	address of cell with modifier
# 	temporary 1 and 2
# temporary 2 is clean after each iteration

start:
add(char val inv) ;
null(inv) ;
out(val:0) ;
loop(char end inc val inv) end()
null(val) inv: start
inc: -1
char: char+1 "Hello, World!\n" end: end
